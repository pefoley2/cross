[MESSAGES CONTROL]

# Disable the message, report, category or checker with the given id(s). You
# can either give multiple identifiers separated by comma (,) or put this
# option multiple times (only on the command line, not in the configuration
# file where it should appear only once).You can also use "--disable=all" to
# disable everything first and then reenable specific checks. For example, if
# you want to run only the similarities checker, you can use "--disable=all
# --enable=similarities". If you want to run only the classes checker, but have
# no Warning level messages displayed, use"--disable=all --enable=classes
# --disable=W"
disable=dict-view-method,coerce-builtin,backtick,unicode-builtin,long-suffix,indexing-exception,old-ne-operator,intern-builtin,no-absolute-import,using-cmp-argument,print-statement,long-builtin,file-builtin,xrange-builtin,suppressed-message,standarderror-builtin,filter-builtin-not-iterating,basestring-builtin,reload-builtin,zip-builtin-not-iterating,reduce-builtin,old-division,metaclass-assignment,cmp-builtin,oct-method,range-builtin-not-iterating,coerce-method,import-star-module-level,old-raise-syntax,raw_input-builtin,next-method-called,cmp-method,buffer-builtin,getslice-method,round-builtin,hex-method,input-builtin,unpacking-in-except,dict-iter-method,useless-suppression,old-octal-literal,setslice-method,execfile-builtin,parameter-unpacking,raising-string,nonzero-method,delslice-method,map-builtin-not-iterating,apply-builtin,unichr-builtin,missing-docstring,too-few-public-methods

[REPORTS]

# Tells whether to display a full report or only the messages
reports=no

[FORMAT]

# Maximum number of characters on a single line.
max-line-length=100
